<h1>Create a Team and Heroes!</h1>

<form method="post" action="/teams">
  Team Name: <input type="text" name="team[name]"><br>
  Team Motto: <input type="text" name="team[motto]">
  
  <h1>Hero 1</h1>
  Hero's Name: <input id="member1_name" type="text" name="team[members][][name]"> <br> 
  Hero's Power: <input id="member1_power" type="text" name="team[members][][power]"> <br>
  Hero's Biography <input id="member1_bio" type="text" name="team[members][][bio]"> <br>
  
  <h1>Hero 2</h1>
  Hero's Name: <input id="member2_name" type="text" name="team[members][][name]"> <br> 
  Hero's Power: <input id="member2_power" type="text" name="team[members][][power]"> <br>
  Hero's Biography: <input id="member2_bio" type="text" name="team[members][][bio]"> <br>
  
  <h1>Hero 3</h1>
  Hero's Name: <input id="member3_name" type="text" name="team[members][][name]"> <br>
  Hero's Power:  <input id="member3_power" type="text" name="team[members][][power]"><br>
  Hero's Biography: <input id="member3_bio" type="text" name="team[members][][bio]"><br> <br>
  
  <!--may need id="Submit" or id="submit"-->
  <input id="submit" type="submit">
  
</form>
